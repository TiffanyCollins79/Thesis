@incollection{BeckmanJohnEtienne2021IA,
	abstract = {William Herschel, the discoverer of Uranus, first detected infrared (IR) radiation from the Sun in 1800, and the Moon’s infrared was detected and measured by Piazzi Smyth in 1856 from a mountain in Tenerife. Astronomers were using infrared techniques in the first half of the XX century, but the great impulse to infrared astronomy was the development of IR detectors in the 1960s. The IR is powerful because we can detect cool objects, stars in formation, planets around other stars, and because the IR penetrates the dense clouds of dust found in the gas populated by newly forming stars. This dust concentrates in the Galactic plane and IR was needed to detect and verify the presence of the supermassive black hole at the Galactic centre. The atmosphere has “windows” of transparency through which some IR photons reach the ground, but for much of the IR, observations from aircraft and satellites are needed. The rings of Uranus were found from an airborne observatory in the IR. Now we regularly map Galaxies in the IR from their molecules and dust clouds, with techniques extending towards the edge of the radio spectrum. And the expansion of the universe causes the radiation from the most distant galaxies to reach us in the IR. Soon our horizons will be widened by the launching of the great James Webb IR space observatory.},
	author = {Beckman, John Etienne},
	address = {Cham},
